{
  "name": "@isyfact/isy-angular-widgets",
  "version": "0.9.0",
  "description": "contains specific widgets for the development of apps in the public sector and applies the default Isyfact Theme.",
  "author": {
    "name": "IsyFact-Team",
    "email": "isyfact@bva.bund.de",
    "url": "https://www.bva.bund.de"
  },
  "license": "Apache-2.0",
  "homepage": "http://isyfact.de",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build isy-angular-widgets && ng build isy-angular-widgets-demo && npm run build:schematics",
    "postbuild": "cd ./projects/isy-angular-widgets && copyfiles schematics/*/schema.json schematics/*/files/** schematics/collection.json ../../dist/isy-angular-widgets/",
    "build:schematics": "tsc -p ./projects/isy-angular-widgets/tsconfig.schematics.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch=false",
    "lint": "ng lint",
    "e2e": "npx testcafe chrome:headless ./projects/isy-angular-widgets-demo/src/test/e2e/*",
    "compodoc:build": "compodoc -p tsconfig.doc.json --disablePrivate --disableInternal",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s --disablePrivate --disableInternal",
    "compodoc:serve": "compodoc -s"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/cdk": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@compodoc/compodoc": "^1.1.19",
    "@isyfact/eslint-plugin": "^1.2.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "chart.js": "^4.3.3",
    "flag-icons": "^6.9.5",
    "primeflex": "^3.3.0",
    "primeicons": "^6.0.1",
    "primeng": "^16.2.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.1",
    "zone.js": "^0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular-eslint/builder": "16.1.0",
    "@angular-eslint/eslint-plugin": "16.1.0",
    "@angular-eslint/eslint-plugin-template": "^16.1.0",
    "@angular-eslint/schematics": "16.1.0",
    "@angular-eslint/template-parser": "16.1.0",
    "@angular/cli": "~16.2.0",
    "@angular/compiler-cli": "^16.2.0",
    "@types/jasmine": "~4.3.5",
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.47.0",
    "eslint-plugin-jsdoc": "^46.4.6",
    "jasmine-core": "~5.1.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "moment": "^2.29.4",
    "ng-mocks": "^14.11.0",
    "ng-packagr": "^16.2.0",
    "ngx-translate-testing": "^7.0.0",
    "typescript": "~5.1.6"
  }
}
