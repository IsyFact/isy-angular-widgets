{
  "name": "@isyfact/isy-angular-widgets",
  "version": "0.0.0",
  "description": "contains specific widgets for the development of apps in the public sector and applies the default Isyfact Theme.",
  "author": {
    "name": "IsyFact-Team",
    "email": "isyfact@bva.bund.de",
    "url": "https://www.bva.bund.de"
  },
  "license": "Apache-2.0",
  "homepage": "http://isyfact.de",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build isy-angular-widgets && ng build isy-angular-widgets-demo && npm run build:schematics",
    "build:widgets_lib": "ng build isy-angular-widgets && npm run build:schematics && npm run postbuild",
    "build:widgets_demo": "ng build isy-angular-widgets-demo",
    "build:schematics": "tsc -p ./projects/isy-angular-widgets/tsconfig.schematics.json",
    "postbuild": "cd ./projects/isy-angular-widgets && copyfiles schematics/*/schema.json schematics/*/files/** schematics/collection.json ../../dist/isy-angular-widgets/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch=false",
    "lint": "ng lint",
    "e2e": "npx testcafe chrome:headless ./projects/isy-angular-widgets-demo/src/test/e2e/*",
    "compodoc:build": "compodoc -p tsconfig.doc.json -d ./docs --disablePrivate --disableInternal",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -d ./docs -s --disablePrivate --disableInternal",
    "compodoc:serve": "compodoc -s -d ./docs",
    "prettier:check": "prettier . --check",
    "prettier:fix": "prettier . --write"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.6",
    "@angular/cdk": "^17.0.3",
    "@angular/common": "^17.0.6",
    "@angular/compiler": "^17.0.6",
    "@angular/core": "^17.0.6",
    "@angular/forms": "^17.0.6",
    "@angular/platform-browser": "^17.0.6",
    "@angular/platform-browser-dynamic": "^17.0.6",
    "@angular/router": "^17.0.6",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "chart.js": "^4.4.1",
    "date-fns": "^2.30.0",
    "flag-icons": "^7.0.3",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primeng": "^17.0.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.6",
    "@angular-eslint/builder": "17.1.1",
    "@angular-eslint/eslint-plugin": "17.1.1",
    "@angular-eslint/eslint-plugin-template": "^17.1.1",
    "@angular-eslint/schematics": "17.1.1",
    "@angular-eslint/template-parser": "17.1.1",
    "@angular/cli": "~17.0.6",
    "@angular/compiler-cli": "^17.0.6",
    "@compodoc/compodoc": "^1.1.23",
    "@isyfact/eslint-plugin": "^1.4.1",
    "@ngneat/spectator": "^15.0.1",
    "@types/date-fns": "^2.6.0",
    "@types/jasmine": "~5.1.4",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "copyfiles": "^2.4.1",
    "eslint": "^8.55.0",
    "eslint-plugin-editorconfig": "^4.0.3",
    "eslint-plugin-jsdoc": "^46.9.0",
    "jasmine-core": "~5.1.1",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-mocks": "^14.12.1",
    "ng-packagr": "^17.0.2",
    "ngx-translate-testing": "^7.0.0",
    "prettier": "^3.1.0",
    "typescript": "~5.2.2"
  }
}
